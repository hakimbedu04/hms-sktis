<properties>
<property id="817" name="CommandTimeout" dataType="System.Int32" state="default" isArray="false" description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out." typeConverter="" UITypeEditor="" containsID="false" expressionType="None">0</property>
<property id="818" name="OpenRowset" dataType="System.String" state="default" isArray="false" description="Specifies the name of the database object used to open a rowset." typeConverter="" UITypeEditor="" containsID="false" expressionType="None">[dbo].[MstPlantEmpUpd]</property>
<property id="819" name="OpenRowsetVariable" dataType="System.String" state="default" isArray="false" description="Specifies the variable that contains the name of the database object used to open a rowset." typeConverter="" UITypeEditor="" containsID="false" expressionType="None"></property>
<property id="820" name="SqlCommand" dataType="System.String" state="default" isArray="false" description="The SQL command to be executed." typeConverter="" UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91" containsID="false" expressionType="None">SELECT
    E.EmployeeID,
    E.EmployeeNumber,
    E.LocationCode,
    E.GroupCode,
    E.UnitCode,
    E.EffDate,
    ISNULL(
        S.Shift,
        1
    ) AS Shift,
    A.EmployeeNumber AS EmployeeNumber_Absent,
    A.LocationCode AS LocationCode_Absent,
    A.GroupCode AS GroupCode_Absent,
    A.UnitCode AS UnitCode_Absent,
    A.StartDateAbsent,
    A.EndDateAbsent,
    A.Shift AS Shift_Absent,
    A.AbsentType,
    A.SktAbsentCode,
    A.PayrollAbsentCode,
    A.ePaf,
    A.Attachment,
    A.AttachmentPath
FROM
    MstPlantEmpUpd E INNER JOIN ExePlantWorkerAbsenteeism A
        ON E.EmployeeID = A.EmployeeID
    AND E.EffDate &gt;= A.StartDateAbsent
    AND E.EffDate &lt; A.EndDateAbsent LEFT JOIN PlanPlantGroupShift S
        ON S.LocationCode = E.LocationCode
    AND S.GroupCode = E.GroupCode
    AND S.UnitCode = E.UnitCode
    AND E.EffDate &gt;= S.StartDate
    AND E.EffDate &lt; S.EndDate</property>
